APP_NAME=Mordecai Workers
NODE_ENV=development
DB_ENABLED=true

# Database
DB_NAME=your_db
DB_USER=your_user
DB_PASSWORD=your_password
DB_HOST=your_host
DB_PORT=5432
DB_SSL=true
DB_SYNC_MODE=false

# Redis (queues)
REDIS_URL=redis://127.0.0.1:6379

# Scheduler/worker tuning
SCHEDULER_LIMIT=500
SCHEDULER_PER_TENANT_LIMIT=10
SCHEDULER_COOLDOWN_MINUTES=360
SCHEDULER_TENANT_ID=
WORKER_CONCURRENCY=5
WORKER_COOLDOWN_MINUTES=360
WORKER_ATTEMPTS=3

# Twilio outbound call config
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_FROM_NUMBER=+12135550123
TWILIO_VOICE_URL=https://mordecai-api.duckdns.org/api/v1/twilio/voice

# Signed voice-context contract for API /twilio/voice
CALL_CONTEXT_HMAC_SECRET=replace_with_strong_random_secret
CALL_CONTEXT_SIGNATURE_VERSION=1
CALL_CONTEXT_TTL_SECONDS=600
